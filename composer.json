{
    "name": "demokn/yii2-easy-wechat",
    "description": "WeChat SDK for yii2 framework, 基于 overtrue/wechat",
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "demokn",
            "email": "demo.knyang@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "yiisoft/yii2": "~2.0.14",
        "overtrue/wechat": "~4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.16",
        "phpunit/phpunit": "~8.5"
    },
    "autoload": {
        "psr-4": {
            "demokn\\easywechat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "demokn\\easywechat\\tests\\": "tests/"
        }
    }
}
